from netmiko import ConnectHandler

iosv_l2_s4 = {
    'device_type': 'cisco_ios',
    'ip': '192.168.0.211',
    'username': 'cisco',
    'password': 'cisco',
}

net_connect = ConnectHandler(**iosv_l2_s4)

getconfig = net_connect.send_command('show run')
print (getconfig)

file = open('config.txt', 'w')
file.write(getconfig)
file.close()
